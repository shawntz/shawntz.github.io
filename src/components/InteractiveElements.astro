    if (totalClicksEl) totalClicksEl.textContent = sessionStats.totalClicks;
    if (confettiEl) confettiEl.textContent = sessionStats.confettiBursts;
    if (easterEggsEl) easterEggsEl.textContent = sessionStats.easterEggsFound;
    
    if (timeSpentEl) {
      const timeSpent = Math.floor((Date.now() - sessionStats.sessionStartTime) / 1000);
      timeSpentEl.textContent = timeSpent + 's';
