    const color = colors[Math.floor(Math.random() * colors.length)];
    
    // Size variations for more realism
    const baseSize = Math.random() * 8 + 4; // 4-12px
    let width = baseSize;
    let height = baseSize;
    
    if (shape === 'rectangle') {
      width = baseSize * 1.5;
      height = baseSize * 0.7;
    } else if (shape === 'triangle') {
      // Triangle using CSS borders
      const size = baseSize;
      confetti.style.borderLeft = `${size/2}px solid transparent`;
      confetti.style.borderRight = `${size/2}px solid transparent`;
      confetti.style.borderBottom = `${size}px solid ${color}`;
      confetti.classList.add('confetti-triangle');
    } else {
      confetti.style.backgroundColor = color;
      confetti.style.width = width + 'px';
      confetti.style.height = height + 'px';
      confetti.classList.add(`confetti-${shape}`);
    }
    
    // Position with some randomness around burst point
    const spread = 200; // pixels of spread from burst point
    const startX = burst.x + (Math.random() - 0.5) * spread;
    const startY = burst.y;
    
    confetti.style.left = startX + 'vw';
    confetti.style.top = startY + 'vh';
    
    // Physics-based properties
    const fallDuration = Math.random() * 2000 + 2000; // 2-4 seconds
    const horizontalDrift = (Math.random() - 0.5) * 300; // drift left/right
    const rotationIntensity = Math.random() * 3 + 1; // rotation speed multiplier
    
    // Apply realistic animation
    const animationNames = ['confetti-realistic-fall', 'confetti-realistic-fall-2', 'confetti-realistic-fall-3'];
    const selectedAnimation = animationNames[Math.floor(Math.random() * animationNames.length)];
    
    confetti.style.animation = `${selectedAnimation} ${fallDuration}ms cubic-bezier(0.25, 0.46, 0.45, 0.94) forwards`;
    confetti.style.animationDelay = Math.random() * 100 + 'ms';
    
    // Add some wind effect with transform
    confetti.style.transform = `translateX(${horizontalDrift}px)`;
    
    // Gravity and air resistance simulation
    let currentY = parseFloat(confetti.style.top);
    let velocityY = Math.random() * 2 + 1;
    let velocityX = (Math.random() - 0.5) * 4;
    let rotation = 0;
    let rotationSpeed = (Math.random() - 0.5) * 20;
    
    document.body.appendChild(confetti);
    
    // Physics simulation
    const physicsInterval = setInterval(() => {
      currentY += velocityY;
      velocityY += 0.15; // gravity
      velocityX *= 0.99; // air resistance
      rotation += rotationSpeed;
      rotationSpeed *= 0.99; // rotation damping
      
      // Add some wobble
      const wobble = Math.sin(currentY / 30) * 2;
      
      confetti.style.transform = `
        translateY(${currentY}px) 
        translateX(${parseFloat(confetti.style.left) + velocityX * 2 + wobble}px)
        rotate(${rotation}deg)
        rotateY(${rotation * 1.5}deg)
      `;
      
      // Remove when off screen
      if (currentY > window.innerHeight + 100) {
        clearInterval(physicsInterval);
        if (confetti.parentNode) {
          confetti.parentNode.removeChild(confetti);
        }
      }
    }, 16); // ~60fps
    
    // Cleanup after max duration
    setTimeout(() => {
      clearInterval(physicsInterval);
      if (confetti.parentNode) {
        confetti.parentNode.removeChild(confetti);
      }
    }, fallDuration + 500);
  }
  
