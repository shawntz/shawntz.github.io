    const statsPanel = document.getElementById('interaction-stats');
    if (!statsPanel) return;
    
    updateStatsDisplay();
    statsPanel.classList.remove('hidden');
  }
  
  function hideStats() {
    const statsPanel = document.getElementById('interaction-stats');
    if (statsPanel) {
      statsPanel.classList.add('hidden');
