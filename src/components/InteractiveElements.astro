  document.addEventListener('astro:before-swap', function() {
    // Hide any active modals during transition
    const easterEggModal = document.getElementById('easter-egg-modal');
    const clickCounter = document.getElementById('click-counter');
    
    if (easterEggModal) easterEggModal.classList.add('hidden');
    if (clickCounter) clickCounter.classList.add('hidden');
  });
  
  function initializeInteractiveElements() {
    // Load persistent session data
    loadSessionData();
    
    // Track page visit
    trackPageVisit();
    
    // Restore click count from session
    clickCount = sessionStats.totalClicks;
    
    // Show persistent elements based on session
    if (clickCount >= 5) {
      showClickCounter();
      updateClickProgress();
    }
    
    console.log('üéÆ Interactive elements initialized');
    console.log(`üìä Session Stats: ${sessionStats.totalClicks} clicks, ${sessionStats.pagesVisited.length} pages visited`);
    
    // Add some random personality
    const personalityMessages = [
      'üß† Welcome back to my digital brain!',
      'üíª Built with code and coffee',
      'üî¨ Science meets software',
      'üöÄ Exploring the mind, one neuron at a time',
      `üéØ You've clicked ${sessionStats.totalClicks} times across your session!`,
      `üìö You've explored ${sessionStats.pagesVisited.length} pages so far!`
    ];
    
    const randomMessage = personalityMessages[Math.floor(Math.random() * personalityMessages.length)];
    console.log(randomMessage);
  }
  
  function loadSessionData() {
    try {
      const stored = localStorage.getItem('interactiveSession');
      if (stored) {
        const data = JSON.parse(stored);
        Object.assign(sessionStats, data);
      }
    } catch (error) {
      console.warn('Could not load session data:', error);
    }
  }
  
  function saveSessionData() {
    try {
      sessionStats.lastActive = Date.now();
      localStorage.setItem('interactiveSession', JSON.stringify(sessionStats));
    } catch (error) {
      console.warn('Could not save session data:', error);
    }
  }
  
  function trackPageVisit() {
    const currentPath = window.location.pathname;
    if (!sessionStats.pagesVisited.includes(currentPath)) {
      sessionStats.pagesVisited.push(currentPath);
      sessionStats.totalSessions++;
      saveSessionData();
    }
    sessionStats.currentPage = currentPath;
  }
  
  function setupScrollProgress() {
    const progressBar = document.querySelector('.progress-bar');
    const progressText = document.querySelector('.progress-percentage');
    
    if (!progressBar || !progressText) return;
    
    function updateScrollProgress() {
      const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
      const scrollHeight = document.documentElement.scrollHeight - window.innerHeight;
      const scrollPercent = (scrollTop / scrollHeight) * 100;
      
      progressBar.style.width = Math.min(100, Math.max(0, scrollPercent)) + '%';
      progressText.textContent = Math.round(scrollPercent) + '%';
    }
    
    window.addEventListener('scroll', updateScrollProgress);
    updateScrollProgress();
  }
  
  function setupFloatingElements() {
    const container = document.getElementById('floating-elements');
    if (!container) return;
    
    floatingElements = Array.from(container.querySelectorAll('.float-item'));
    
    // Mouse movement effect
    document.addEventListener('mousemove', function(e) {
      const mouseX = e.clientX / window.innerWidth;
      const mouseY = e.clientY / window.innerHeight;
      
      floatingElements.forEach(element => {
        const speed = parseFloat(element.dataset.speed) || 0.5;
        const x = (mouseX - 0.5) * 150 * speed;
        const y = (mouseY - 0.5) * 150 * speed;
        
        element.style.transform = `translate(${x}px, ${y}px) rotate(${x / 8}deg)`;
        
        // Enhanced opacity based on element type
        const baseOpacity = element.classList.contains('primary') ? 0.2 : 
                           element.classList.contains('secondary') ? 0.12 : 0.04;
        const hoverBoost = Math.abs(mouseX - 0.5) + Math.abs(mouseY - 0.5);
        const maxOpacity = element.classList.contains('primary') ? 0.7 : 
                          element.classList.contains('secondary') ? 0.3 : 0.1;
        
        element.style.opacity = Math.min(maxOpacity, baseOpacity + hoverBoost * 1.5);
      });
    });
    
    // Apple Music-style visualizer effect
    let audioContext, analyser;
    const frequencyBands = new Array(floatingElements.length).fill(0);
    
    // Simulate audio frequency data for visualizer effect
    function simulateAudioData() {
      return Array.from({ length: 32 }, () => Math.random() * 255);
    }
    
    // Music visualizer animation
    function updateVisualizerEffect() {
      const audioData = simulateAudioData();
      
      floatingElements.forEach((element, index) => {
        if (element.classList.contains('ghost')) return; // Skip ghost elements
        
        const frequency = audioData[index % audioData.length] || 0;
        const normalizedFreq = frequency / 255;
        
        // Create pulsing effect based on "frequency"
        const pulseScale = 1 + (normalizedFreq * 0.3);
        const brightnessBoost = 1 + (normalizedFreq * 0.4);
        const opacityBoost = element.classList.contains('primary') ? 
          0.12 + (normalizedFreq * 0.2) : 0.08 + (normalizedFreq * 0.15);
        
        // Apply visualizer effects
        const currentTransform = element.style.transform || '';
        const hasExistingScale = currentTransform.includes('scale');
        
        if (!hasExistingScale) {
          element.style.transform += ` scale(${pulseScale})`;
        }
        
        element.style.filter = `blur(0.2px) brightness(${brightnessBoost}) saturate(1.3)`;
        element.style.opacity = Math.min(opacityBoost, 0.3);
        
        // Add subtle color shifts
        const hueShift = (normalizedFreq * 60) + (Date.now() / 100 % 360);
        element.style.filter += ` hue-rotate(${hueShift}deg)`;
      });
      
      requestAnimationFrame(updateVisualizerEffect);
    }
    
    // Start the visualizer effect
    updateVisualizerEffect();
    
    // Random bass drop effect
    setInterval(() => {
      if (Math.random() < 0.15) { // 15% chance for "bass drop"
        floatingElements.forEach((element, index) => {
          if (element.classList.contains('ghost')) return;
          
          const delay = index * 50;
          setTimeout(() => {
            element.style.animation = 'musicPulse 0.3s ease-out';
            element.style.transform += ' scale(1.4)';
            element.style.filter = 'blur(0px) brightness(1.5) saturate(2)';
            element.style.opacity = '0.4';
            
            setTimeout(() => {
              element.style.transform = element.style.transform.replace(' scale(1.4)', '');
              element.style.filter = '';
              element.style.opacity = '';
              element.style.animation = '';
            }, 300);
          }, delay);
        });
      }
    }, 8000);
  }
  
  function setupKonamiCode() {
    document.addEventListener('keydown', function(e) {
      konamiSequence.push(e.code);
      
      // Keep only the last 10 keys
      if (konamiSequence.length > 10) {
        konamiSequence.shift();
      }
      
      // Check if sequence matches Konami code
      if (konamiSequence.length === 10 && 
          konamiSequence.every((key, index) => key === konamiCode[index])) {
        triggerEasterEgg();
        konamiSequence = []; // Reset sequence
      }
    });
  }
  
  function setupClickableElements() {
    let consecutiveClicks = 0;
    let clickTimer;
    
    document.addEventListener('click', function(e) {
      clickCount++;
      consecutiveClicks++;
      sessionStats.totalClicks++;
      
      // Save to session periodically
      if (sessionStats.totalClicks % 5 === 0) {
        saveSessionData();
      }
      
      // Clear previous timer
      if (clickTimer) {
        clearTimeout(clickTimer);
      }
      
      // Update click progress and show counter after 5 clicks
      if (clickCount >= 5) {
        showClickCounter();
        updateClickProgress();
      }
      
      // Check for achievements
      checkAchievements();
      
      // Party mode for rapid clicking
      if (consecutiveClicks >= 5) {
        activatePartyMode();
        sessionStats.confettiBursts++;
        consecutiveClicks = 0;
      }
      
      // Show stats panel after significant interaction
      if (clickCount === 50) {
        showStats();
      }
      
      // Cross-page achievements
      if (sessionStats.totalClicks === 100) {
        showAchievement('Cross-Page Explorer!', 'You\'ve clicked 100+ times across pages! üåü');
      }
      
      // Reset consecutive clicks after inactivity
      clickTimer = setTimeout(() => {
        consecutiveClicks = 0;
      }, 1000);
      
      // Add hover effect to clicked element
      const target = e.target.closest('.neobrutalist-card, .repo-card, .citation');
      if (target && !target.classList.contains('clickable-hover')) {
        target.classList.add('clickable-hover');
      }
    });
  }
  
  function updateClickProgress() {
    const progressBar = document.getElementById('click-progress');
    const nextMilestone = gamificationMilestones.find(m => !m.achieved);
    
    if (progressBar && nextMilestone) {
      const progress = (clickCount / nextMilestone.clicks) * 100;
      progressBar.style.width = Math.min(100, progress) + '%';
    }
  }
  
  function checkAchievements() {
    gamificationMilestones.forEach(milestone => {
      if (!milestone.achieved && clickCount >= milestone.clicks) {
        milestone.achieved = true;
        sessionStats.achievements.push(milestone);
        showAchievement(milestone.title, milestone.description);
      }
    });
  }
  
