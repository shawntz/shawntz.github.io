  let floatingElements = [];
  
  // Enhanced gamification tracking with persistence
  let sessionStats = JSON.parse(localStorage.getItem('interactiveSession') || JSON.stringify({
    totalClicks: 0,
    confettiBursts: 0,
    easterEggsFound: 0,
    sessionStartTime: Date.now(),
    achievements: [],
    totalSessions: 0,
    pagesVisited: [],
    currentPage: window.location.pathname
  }));
  
  let gamificationMilestones = [
