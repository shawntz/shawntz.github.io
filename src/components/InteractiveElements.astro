    { clicks: 25, title: "Click Enthusiast", description: "You love clicking things! üëÜ", achieved: false },
    { clicks: 50, title: "Party Starter", description: "You've triggered confetti! üéâ", achieved: false },
    { clicks: 100, title: "Super Clicker", description: "100 clicks achieved! üíØ", achieved: false },
    { clicks: 200, title: "Click Master", description: "You're unstoppable! üöÄ", achieved: false }
  ];
  
  document.addEventListener('DOMContentLoaded', function() {
    initializeInteractiveElements();
    setupScrollProgress();
    setupFloatingElements();
    setupKonamiCode();
