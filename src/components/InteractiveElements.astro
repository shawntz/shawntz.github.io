---
// Interactive personality elements for the website
interface Props {
  enableKonamiCode?: boolean;
  enableClickableElements?: boolean;
  enableScrollAnimations?: boolean;
}

const {
  enableKonamiCode = true,
  enableClickableElements = true,  
  enableScrollAnimations = true
} = Astro.props;
---

<!-- Enhanced floating elements with better visibility and responsiveness -->
<div class="floating-elements" id="floating-elements">
  <!-- Core elements - main positions -->
  <div class="float-item brain primary" data-speed="0.5">üß†</div>
  <div class="float-item code primary" data-speed="0.8">üíª</div>
  <div class="float-item data primary" data-speed="0.6">üìä</div>
  <div class="float-item coffee primary" data-speed="0.4">‚òï</div>
  <div class="float-item lightbulb primary" data-speed="0.7">üí°</div>
  
  <!-- Secondary elements for better coverage -->
  <div class="float-item rocket secondary" data-speed="0.3">üöÄ</div>
  <div class="float-item science secondary" data-speed="0.9">üî¨</div>
  <div class="float-item phone secondary" data-speed="0.5">üì±</div>
  <div class="float-item chart secondary" data-speed="0.6">üìà</div>
  <div class="float-item memory secondary" data-speed="0.4">üßÆ</div>
  <div class="float-item spark secondary" data-speed="0.8">‚ú®</div>
  <div class="float-item gear secondary" data-speed="0.7">‚öôÔ∏è</div>
  
  <!-- Subtle background elements -->
  <div class="float-item ghost ghost1" data-speed="0.2">üß†</div>
  <div class="float-item ghost ghost2" data-speed="0.3">üíª</div>
  <div class="float-item ghost ghost3" data-speed="0.25">üìä</div>
  <div class="float-item ghost ghost4" data-speed="0.35">‚òï</div>
  <div class="float-item ghost ghost5" data-speed="0.2">üî¨</div>
  <div class="float-item ghost ghost6" data-speed="0.4">üöÄ</div>
</div>

<!-- Scroll progress indicator -->
<div class="scroll-progress" id="scroll-progress">
  <div class="progress-bar"></div>
  <div class="progress-text">
    <span class="progress-percentage">0%</span>
    <span class="progress-label">Reading Progress</span>
  </div>
</div>

<!-- Hidden Easter egg content -->
<div class="easter-egg-modal hidden" id="easter-egg-modal">
  <div class="modal-content neobrutalist-card">
    <div class="modal-header">
      <h3>üéâ You found the secret!</h3>
      <button class="close-modal" onclick="closeEasterEgg()">‚úï</button>
    </div>
    <div class="modal-body">
      <div class="secret-content">
        <div class="secret-animation">
          <div class="dancing-elements">
            <span class="dance">üß†</span>
            <span class="dance">üíª</span>
            <span class="dance">üî¨</span>
            <span class="dance">üì±</span>
          </div>
        </div>
        <h4>Developer Mode Activated!</h4>
        <p>
          You've unlocked some fun facts about this site:
        </p>
        <ul class="fun-facts">
          <li>Built with Astro.js and lots of coffee ‚òï</li>
          <li>Features a custom neobrutalist design system</li>
          <li>Uses Slack-inspired colors (because I work there! üíú)</li>
          <li>Has hidden analytics tracking for nerdy insights üìà</li>
          <li>Contains exactly {Math.floor(Math.random() * 1000) + 5000} lines of code</li>
        </ul>
        <div class="secret-badge">
          <span class="badge-text">üèÜ Code Explorer</span>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Enhanced Gamification System -->
<div class="gamification-system">
  <!-- Click counter -->
  <div class="click-counter hidden" id="click-counter">
    <div class="counter-content">
      <span class="counter-emoji">üëÜ</span>
      <span class="counter-text">Clicks: <span class="count">0</span></span>
      <div class="counter-progress">
        <div class="progress-fill" id="click-progress"></div>
      </div>
      <span class="counter-hint">Keep clicking for surprises! üéâ</span>
    </div>
  </div>
  
  <!-- Achievement notifications -->
  <div class="achievement-popup hidden" id="achievement-popup">
    <div class="achievement-content">
      <div class="achievement-icon">üèÜ</div>
      <div class="achievement-text">
        <h4 class="achievement-title">Achievement Unlocked!</h4>
        <p class="achievement-description">You've discovered the click party!</p>
      </div>
      <div class="achievement-close">√ó</div>
    </div>
  </div>
  
  <!-- Secret developer badge -->
  <div class="dev-badge hidden" id="dev-badge">
    <div class="badge-content">
      <span class="badge-icon">üî•</span>
      <span class="badge-text">Code Explorer</span>
      <div class="badge-subtitle">Found hidden features</div>
    </div>
  </div>
  
  <!-- Interactive stats -->
  <div class="interaction-stats hidden" id="interaction-stats">
    <div class="stats-header">
      <h4>üéÆ Session Stats</h4>
      <button class="stats-close" onclick="hideStats()">√ó</button>
    </div>
    <div class="stats-grid">
      <div class="stat-item">
        <div class="stat-number" id="total-clicks">0</div>
        <div class="stat-label">Total Clicks</div>
      </div>
      <div class="stat-item">
        <div class="stat-number" id="confetti-triggered">0</div>
        <div class="stat-label">Confetti Bursts</div>
      </div>
      <div class="stat-item">
        <div class="stat-number" id="easter-eggs-found">0</div>
        <div class="stat-label">Easter Eggs</div>
      </div>
      <div class="stat-item">
        <div class="stat-number" id="time-spent">0s</div>
        <div class="stat-label">Time Exploring</div>
      </div>
    </div>
  </div>
</div>

<style>
  .floating-elements {
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    pointer-events: none;
    z-index: -1; /* Behind all content */
    overflow: hidden;
    mix-blend-mode: multiply; /* Better text visibility */
  }
  
  /* Fade out emojis in text areas */
  .floating-elements::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: radial-gradient(circle at center, 
      transparent 20%, 
      rgba(255, 255, 255, 0.1) 40%,
      rgba(255, 255, 255, 0.3) 60%,
      rgba(255, 255, 255, 0.5) 80%
    );
    z-index: 1;
    pointer-events: none;
  }
  
  .float-item {
    position: absolute;
    font-size: 3rem;
    opacity: 0.08;
    transition: all 0.3s ease;
    animation: musicVisualizerFloat 4s ease-in-out infinite;
    user-select: none;
    z-index: -1;
    will-change: transform, opacity, filter;
    filter: blur(0.5px) brightness(0.9);
  }
  
  .float-item.primary {
    font-size: 3.5rem;
    opacity: 0.12;
    z-index: -1;
    animation: musicVisualizerPrimary 3s ease-in-out infinite;
  }
  
  .float-item.secondary {
    font-size: 2.5rem;
    opacity: 0.08;
    z-index: -2;
    animation: musicVisualizerSecondary 5s ease-in-out infinite;
  }
  
  .float-item.ghost {
    font-size: 4rem;
    opacity: 0.03;
    z-index: -3;
    animation: musicVisualizerGhost 7s ease-in-out infinite;
  }
  
  /* Primary elements - main focal points */
  .float-item.brain {
    top: 15%;
    left: 8%;
    animation-delay: 0s;
  }
  
  .float-item.code {
    top: 65%;
    right: 12%;
    animation-delay: 1.5s;
  }
  
  .float-item.data {
    top: 35%;
    left: 3%;
    animation-delay: 2.5s;
  }
  
  .float-item.coffee {
    top: 85%;
    right: 25%;
    animation-delay: 3.5s;
  }
  
  .float-item.lightbulb {
    top: 25%;
    right: 3%;
    animation-delay: 4.5s;
  }
  
  /* Secondary elements for better coverage */
  .float-item.rocket {
    top: 8%;
    right: 30%;
    animation-delay: 1s;
  }
  
  .float-item.science {
    top: 75%;
    left: 15%;
    animation-delay: 2s;
  }
  
  .float-item.phone {
    top: 50%;
    right: 35%;
    animation-delay: 3s;
  }
  
  .float-item.chart {
    top: 45%;
    left: 25%;
    animation-delay: 4s;
  }
  
  .float-item.memory {
    top: 10%;
    left: 45%;
    animation-delay: 5s;
  }
  
  .float-item.spark {
    top: 70%;
    right: 8%;
    animation-delay: 5.5s;
  }
  
  .float-item.gear {
    top: 55%;
    left: 40%;
    animation-delay: 6s;
  }
  
  /* Ghost elements - subtle background */
  .float-item.ghost1 {
    top: 5%;
    left: 60%;
    animation-delay: 1s;
  }
  
  .float-item.ghost2 {
    top: 95%;
    left: 35%;
    animation-delay: 2s;
  }
  
  .float-item.ghost3 {
    top: 20%;
    right: 50%;
    animation-delay: 3s;
  }
  
  .float-item.ghost4 {
    top: 80%;
    left: 60%;
    animation-delay: 4s;
  }
  
  .float-item.ghost5 {
    top: 40%;
    right: 60%;
    animation-delay: 5s;
  }
  
  .float-item.ghost6 {
    top: 60%;
    left: 70%;
    animation-delay: 6s;
  }
  
  /* Apple Music Visualizer-Style Animations */
  @keyframes musicVisualizerFloat {
    0% {
      transform: translateY(0px) rotate(0deg) scale(1);
      opacity: 0.08;
      filter: blur(0.5px) brightness(0.9) hue-rotate(0deg);
    }
    15% {
      transform: translateY(-20px) rotate(3deg) scale(1.1);
      opacity: 0.15;
      filter: blur(0.3px) brightness(1.1) hue-rotate(15deg);
    }
    30% {
      transform: translateY(-40px) rotate(-2deg) scale(0.95);
      opacity: 0.12;
      filter: blur(0.4px) brightness(0.95) hue-rotate(30deg);
    }
    50% {
      transform: translateY(-25px) rotate(5deg) scale(1.05);
      opacity: 0.18;
      filter: blur(0.2px) brightness(1.15) hue-rotate(45deg);
    }
    65% {
      transform: translateY(-15px) rotate(-1deg) scale(0.9);
      opacity: 0.1;
      filter: blur(0.6px) brightness(0.85) hue-rotate(30deg);
    }
    80% {
      transform: translateY(-35px) rotate(4deg) scale(1.08);
      opacity: 0.16;
      filter: blur(0.3px) brightness(1.05) hue-rotate(15deg);
    }
    100% {
      transform: translateY(0px) rotate(0deg) scale(1);
      opacity: 0.08;
      filter: blur(0.5px) brightness(0.9) hue-rotate(0deg);
    }
  }
  
  @keyframes musicVisualizerPrimary {
    0% {
      transform: translateY(0px) scale(1) rotate(0deg);
      opacity: 0.12;
      filter: blur(0.3px) brightness(1) saturate(1.2);
    }
    20% {
      transform: translateY(-30px) scale(1.15) rotate(8deg);
      opacity: 0.25;
      filter: blur(0.1px) brightness(1.3) saturate(1.5);
    }
    40% {
      transform: translateY(-50px) scale(0.9) rotate(-5deg);
      opacity: 0.18;
      filter: blur(0.4px) brightness(0.9) saturate(1.1);
    }
    60% {
      transform: translateY(-20px) scale(1.2) rotate(6deg);
      opacity: 0.28;
      filter: blur(0.2px) brightness(1.4) saturate(1.6);
    }
    80% {
      transform: translateY(-40px) scale(0.95) rotate(-3deg);
      opacity: 0.15;
      filter: blur(0.3px) brightness(0.95) saturate(1.2);
    }
    100% {
      transform: translateY(0px) scale(1) rotate(0deg);
      opacity: 0.12;
      filter: blur(0.3px) brightness(1) saturate(1.2);
    }
  }
  
  @keyframes musicVisualizerSecondary {
    0% {
      transform: translateY(0px) scale(1) rotate(0deg);
      opacity: 0.08;
    }
    25% {
      transform: translateY(-15px) scale(1.05) rotate(-4deg);
      opacity: 0.14;
    }
    50% {
      transform: translateY(-35px) scale(0.92) rotate(7deg);
      opacity: 0.1;
    }
    75% {
      transform: translateY(-20px) scale(1.08) rotate(-2deg);
      opacity: 0.16;
    }
    100% {
      transform: translateY(0px) scale(1) rotate(0deg);
      opacity: 0.08;
    }
  }
  
  @keyframes musicVisualizerGhost {
    0% {
      transform: translateY(0px) scale(1);
      opacity: 0.03;
    }
    33% {
      transform: translateY(-10px) scale(1.02);
      opacity: 0.06;
    }
    66% {
      transform: translateY(-25px) scale(0.98);
      opacity: 0.04;
    }
    100% {
      transform: translateY(0px) scale(1);
      opacity: 0.03;
    }
  }
  
  /* Pulse animation for music-like effect */
  @keyframes musicPulse {
    0%, 100% {
      transform: scale(1);
    }
    50% {
      transform: scale(1.03);
    }
  }
  
  /* Enhanced hover effects that respect z-index */
  .float-item:hover {
    animation-play-state: paused;
    transform: scale(1.1) !important;
    opacity: 0.25 !important;
    filter: blur(0px) brightness(1.2) saturate(1.5) !important;
    z-index: -1 !important; /* Still behind content */
  }
  
  .float-item.primary:hover {
    opacity: 0.35 !important;
  }
  
  .scroll-progress {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: rgba(255, 255, 255, 0.9);
    backdrop-filter: blur(10px);
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    z-index: 1000;
    transition: all 0.3s ease;
  }
  
  .progress-bar {
    height: 100%;
    background: linear-gradient(90deg, var(--slack-purple), var(--slack-green), var(--slack-blue));
    width: 0%;
    transition: width 0.2s ease;
  }
  
  .progress-text {
    position: absolute;
    right: 1rem;
    top: 50%;
    transform: translateY(-50%);
    display: flex;
    align-items: center;
    gap: 0.5rem;
    font-size: 0.75rem;
    font-weight: 600;
    color: rgb(var(--black));
    opacity: 0;
    transition: opacity 0.3s ease;
  }
  
  .scroll-progress:hover .progress-text {
    opacity: 1;
  }
  
  .easter-egg-modal {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.8);
    backdrop-filter: blur(10px);
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 10000;
    animation: fadeIn 0.3s ease;
  }
  
  @keyframes fadeIn {
    from { opacity: 0; }
    to { opacity: 1; }
  }
  
  .modal-content {
    background: white;
    max-width: 500px;
    width: 90%;
    max-height: 80vh;
    overflow-y: auto;
    animation: slideUp 0.4s ease;
  }
  
  @keyframes slideUp {
    from {
      transform: translateY(50px);
      opacity: 0;
    }
    to {
      transform: translateY(0);
      opacity: 1;
    }
  }
  
  .modal-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 1.5rem;
    background: linear-gradient(135deg, var(--brutal-yellow), var(--brutal-pink));
    border-bottom: 2px solid #000;
  }
  
  .modal-header h3 {
    margin: 0;
    font-size: 1.5rem;
    font-weight: 700;
    color: #000;
  }
  
  .close-modal {
    background: #000;
    color: white;
    border: none;
    width: 32px;
    height: 32px;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: 700;
    transition: all 0.2s ease;
  }
  
  .close-modal:hover {
    background: #333;
    transform: scale(1.1);
  }
  
  .modal-body {
    padding: 2rem;
  }
  
  .secret-animation {
    text-align: center;
    margin-bottom: 2rem;
  }
  
  .dancing-elements {
    display: flex;
    justify-content: center;
    gap: 1rem;
    margin-bottom: 1rem;
  }
  
  .dance {
    font-size: 2rem;
    animation: dance 0.6s ease infinite alternate;
    display: inline-block;
  }
  
  .dance:nth-child(2) { animation-delay: 0.1s; }
  .dance:nth-child(3) { animation-delay: 0.2s; }
  .dance:nth-child(4) { animation-delay: 0.3s; }
  
  @keyframes dance {
    from {
      transform: translateY(0) rotate(0deg);
    }
    to {
      transform: translateY(-10px) rotate(10deg);
    }
  }
  
  .secret-content h4 {
    text-align: center;
    color: var(--academic-blue);
    margin-bottom: 1rem;
  }
  
  .fun-facts {
    list-style: none;
    padding: 0;
    margin: 1rem 0;
  }
  
  .fun-facts li {
    padding: 0.75rem;
    margin: 0.5rem 0;
    background: linear-gradient(135deg, #f0f9ff, #e0e7ff);
    border: 2px solid #000;
    border-radius: 4px;
    box-shadow: 2px 2px 0px 0px rgba(0,0,0,1);
    display: flex;
    align-items: center;
    gap: 0.5rem;
  }
  
  .fun-facts li::before {
    content: 'üî•';
    font-size: 1.2rem;
  }
  
  .secret-badge {
    text-align: center;
    margin-top: 2rem;
  }
  
  .badge-text {
    display: inline-block;
    background: var(--slack-green);
    color: white;
    padding: 1rem 2rem;
    border: 2px solid #000;
    border-radius: 25px;
    font-weight: 700;
    box-shadow: 4px 4px 0px 0px rgba(0,0,0,1);
    animation: pulse 2s ease infinite;
  }
  
  @keyframes pulse {
    0%, 100% { transform: scale(1); }
    50% { transform: scale(1.05); }
  }
  
  /* Enhanced Gamification Styles */
  .gamification-system {
    position: relative;
    z-index: 1000;
  }
  
  .click-counter {
    position: fixed;
    bottom: 2rem;
    right: 2rem;
    background: linear-gradient(135deg, var(--brutal-cyan), var(--brutal-yellow));
    color: #000;
    padding: 1.5rem;
    border: 3px solid #000;
    border-radius: 12px;
    box-shadow: 6px 6px 0px 0px rgba(0,0,0,1);
    font-weight: 700;
    z-index: 1001;
    animation: bounceIn 0.6s cubic-bezier(0.68, -0.55, 0.265, 1.55);
    min-width: 200px;
  }
  
  @keyframes bounceIn {
    0% {
      transform: scale(0) translateY(100px) rotate(-10deg);
      opacity: 0;
    }
    50% {
      transform: scale(1.15) translateY(-10px) rotate(5deg);
      opacity: 1;
    }
    100% {
      transform: scale(1) translateY(0) rotate(0deg);
      opacity: 1;
    }
  }
  
  .counter-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 0.75rem;
    text-align: center;
  }
  
  .counter-emoji {
    font-size: 2rem;
    animation: bounce 1s ease infinite;
  }
  
  .counter-text {
    font-size: 1.1rem;
    font-weight: 800;
  }
  
  .counter-progress {
    width: 100%;
    height: 6px;
    background: rgba(0,0,0,0.2);
    border-radius: 3px;
    overflow: hidden;
    border: 1px solid #000;
  }
  
  .progress-fill {
    height: 100%;
    background: linear-gradient(90deg, var(--slack-green), var(--slack-purple));
    width: 0%;
    transition: width 0.3s ease;
    border-radius: 2px;
  }
  
  .counter-hint {
    font-size: 0.8rem;
    opacity: 0.8;
    animation: pulse 2s ease infinite;
  }
  
  @keyframes bounce {
    0%, 100% { transform: translateY(0) scale(1); }
    50% { transform: translateY(-8px) scale(1.1); }
  }
  
  @keyframes pulse {
    0%, 100% { opacity: 0.8; }
    50% { opacity: 1; }
  }
  
  /* Achievement Popup */
  .achievement-popup {
    position: fixed;
    top: 2rem;
    right: 2rem;
    background: linear-gradient(135deg, var(--slack-green), #00C851);
    border: 3px solid #000;
    border-radius: 12px;
    box-shadow: 8px 8px 0px 0px rgba(0,0,0,1);
    z-index: 1002;
    animation: slideInRight 0.5s ease;
    max-width: 300px;
  }
  
  @keyframes slideInRight {
    from {
      transform: translateX(100%);
      opacity: 0;
    }
    to {
      transform: translateX(0);
      opacity: 1;
    }
  }
  
  .achievement-content {
    display: flex;
    align-items: center;
    gap: 1rem;
    padding: 1.5rem;
    color: white;
    position: relative;
  }
  
  .achievement-icon {
    font-size: 2.5rem;
    animation: rotate 2s ease infinite;
  }
  
  @keyframes rotate {
    0%, 100% { transform: rotate(0deg) scale(1); }
    50% { transform: rotate(15deg) scale(1.1); }
  }
  
  .achievement-title {
    margin: 0 0 0.5rem 0;
    font-size: 1.1rem;
    font-weight: 800;
  }
  
  .achievement-description {
    margin: 0;
    font-size: 0.9rem;
    opacity: 0.9;
  }
  
  .achievement-close {
    position: absolute;
    top: 0.5rem;
    right: 0.5rem;
    cursor: pointer;
    font-size: 1.2rem;
    font-weight: bold;
    opacity: 0.7;
    transition: opacity 0.2s ease;
  }
  
  .achievement-close:hover {
    opacity: 1;
  }
  
  /* Developer Badge */
  .dev-badge {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: linear-gradient(135deg, var(--slack-purple), #9C27B0);
    color: white;
    padding: 2rem;
    border: 3px solid #000;
    border-radius: 20px;
